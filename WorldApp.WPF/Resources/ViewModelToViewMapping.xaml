<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:WorldApp.WPF.Views"
                    xmlns:viewModels="clr-namespace:WorldApp.WPF.ViewModels">
    <DataTemplate DataType="{x:Type viewModels:AddContinentsViewModel}">
        <views:AddContinentsView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:AddCountryViewModel}">
        <views:AddCountryView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:AddCitiesViewModel}">
        <views:AddCitiesView />
    </DataTemplate>
</ResourceDictionary>